        autoStat = Value
        
        if Value then
            if not selectedStat then
                Rayfield:Notify({Title = "Error", Content = "Pick a stat first!", Duration = 4})
                autoStat = false
                return
            end
            
            Rayfield:Notify({
                Title = "Auto Stats",
                Content = "Upgrading " .. selectedStat .. " forever...",
                Duration = 4
            })
            
            task.spawn(function()
                local remote = game:GetService("ReplicatedStorage")
                    :WaitForChild("Packages")
                    :WaitForChild("_Index")
                    :WaitForChild("sleitnick_knit@1.5.1")
                    :WaitForChild("knit")
                    :WaitForChild("Services")
                    :WaitForChild("LevelService")
                    :WaitForChild("RF")
                    :WaitForChild("PutStat")
                
                while autoStat do
                    pcall(function()
                        remote:InvokeServer(selectedStat, 1)
                    end)
                    
                    task.wait(30)  -- Safe delay ‚Äì make it 0.4+ if kicked
                end
            end)
        end
    end,
})

