local Tab = Window:CreateTab("Auto Quest", 4483362458) -- Title, Image

-- === ANIME DESTROYERS ULTIMATE FARMER - FIXED DETECTION (Higher Threshold + Debug) ===
-- Now detects EVEN IF slightly far (10k studs) + prints distances to console
-- Paste AFTER Rayfield + Window/Tab

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local mobSpawnsFolder = workspace:WaitForChild("MobSpawns")

local autoFarmEnabled = false
local farmDelay = 5
local currentTargetBaseName = nil
local currentIslandFolder = nil

-- FIXED DETECTION: Higher threshold + ALWAYS pick closest (no fail)
local function detectCurrentIsland()
    local char = player.Character
    if not char then 
        print("[DEBUG] No character")
        return nil 
    end
    
    local root = char:FindFirstChild("HumanoidRootPart") 
        or char:FindFirstChild("Torso") 
        or char:FindFirstChild("UpperTorso") 
        or char.PrimaryPart
        or char:FindFirstChildWhichIsA("BasePart")
    if not root then 
        print("[DEBUG] No root part found")
        return nil 
    end
    
    local playerPos = root.Position
    print("[DEBUG] Player pos:", playerPos)
    
    local distances = {}
    local closestFolder = nil
    local minDist = math.huge
    
    for _, islandFolder in ipairs(mobSpawnsFolder:GetChildren()) do
        if islandFolder:IsA("Folder") and #islandFolder:GetChildren() > 0 then
            local folderMinDist = math.huge
            for _, mob in ipairs(islandFolder:GetChildren()) do
                if mob:IsA("BasePart") then
                    local dist = (playerPos - mob.Position).Magnitude
                    if dist < folderMinDist then
                        folderMinDist = dist
                    end
                end
            end
            distances[islandFolder.Name] = folderMinDist
            print("[DEBUG] Dist to " .. islandFolder.Name .. ": " .. math.floor(folderMinDist) .. " studs (" .. #islandFolder:GetChildren() .. " mobs)")
            
            if folderMinDist < minDist then  -- NO THRESHOLD - always pick closest
                minDist = folderMinDist
                closestFolder = islandFolder
            end
        end
    end
    
    print("[DEBUG] Closest: " .. (closestFolder and closestFolder.Name or "NONE") .. " (" .. math.floor(minDist) .. " studs)")
    return closestFolder
end

-- Rest unchanged...
local function getBaseNames()
    if not currentIslandFolder then return {"No island"} end
    
    local baseNamesSet = {}
    for _, child in ipairs(currentIslandFolder:GetChildren()) do
        local cleanName = child.Name:gsub("_%d+$", "")
            :gsub(" %b()", "")
            :gsub("Clone", "")
            :match("^%s*(.-)%s*$")
        if cleanName and #cleanName > 1 then
            baseNamesSet[cleanName] = true
        end
    end
    local options = {}
    for name in pairs(baseNamesSet) do
        table.insert(options, name)
    end
    table.sort(options)
    return #options > 0 and options or {"No mobs"}
end

local function teleportToMob(baseName)
    if not baseName or baseName:find("No") then return false end

    local matches = {}
    for _, child in ipairs(currentIslandFolder:GetChildren()) do
        if child.Name:lower():find(baseName:lower()) then
            table.insert(matches, child)
        end
    end

    if #matches == 0 then return false end

    local target = matches[math.random(1, #matches)]
    local tpCFrame = target.CFrame * CFrame.new(math.random(-4,4), 6, math.random(-6,-3))

    local char = player.Character
    if char then
        pcall(function() char:PivotTo(tpCFrame) end)
    end

    return true
end

local farmThread
local function startFarmLoop()
    if farmThread then task.cancel(farmThread) end
    farmThread = task.spawn(function()
        while autoFarmEnabled do
            teleportToMob(currentTargetBaseName)
            task.wait(farmDelay)
        end
    end)
end

player.CharacterAdded:Connect(function()
    task.wait(4)
    if autoFarmEnabled then
        currentIslandFolder = detectCurrentIsland()
        teleportToMob(currentTargetBaseName)
    end
end)

-- UI (same, but better status)
local StatusLabel = Tab:CreateLabel("üó∫Ô∏è Click Refresh - See F9 for distances")

local Dropdown = Tab:CreateDropdown({
    Name = "Select Mob (Current Island)",
    Options = {},
    CurrentOption = {},
    MultipleOptions = false,
    Flag = "MobType",
    Callback = function(Option)
        currentTargetBaseName = Option[1]
        local island = currentIslandFolder and currentIslandFolder.Name or "None"
        StatusLabel:Set("üéØ " .. (currentTargetBaseName or "None") .. " | üèùÔ∏è " .. island)
    end,
})

Tab:CreateToggle({
    Name = "üöÄ Auto Farm",
    CurrentValue = false,
    Flag = "AutoFarm",
    Callback = function(Value)
        autoFarmEnabled = Value
        if Value then
            if not currentIslandFolder then
                Rayfield:Notify({Title = "‚ùå Refresh first!", Duration = 4})
                return
            end
            if not currentTargetBaseName then
                Rayfield:Notify({Title = "‚ùå Select mob!", Duration = 4})
                return
            end
            local island = currentIslandFolder.Name
            StatusLabel:Set("üî• FARMING " .. currentTargetBaseName:upper() .. " in " .. island)
            Rayfield:Notify({Title = "Farm ON! üéâ", Duration = 5})
            teleportToMob(currentTargetBaseName)
            startFarmLoop()
        else
            if farmThread then task.cancel(farmThread) end
            StatusLabel:Set("‚èπÔ∏è Stopped")
        end
    end,
})

Tab:CreateSlider({
    Name = "‚è±Ô∏è Delay",
    Range = {2, 20},
    Increment = 0.5,
    Suffix = "s",
    CurrentValue = 5,
    Flag = "Delay",
    Callback = function(Value) farmDelay = Value end,
})

Tab:CreateButton({
    Name = "‚ö° TP Once",
    Callback = function()
        if currentTargetBaseName and currentIslandFolder and teleportToMob(currentTargetBaseName) then
            Rayfield:Notify({Title = "‚úÖ TP OK!", Duration = 2})
        else
            Rayfield:Notify({Title = "‚ùå Refresh?", Duration = 3})
        end
    end,
})

Tab:CreateButton({
    Name = "üîÑ Refresh Island (Debug in F9)",
    Callback = function()
        currentIslandFolder = detectCurrentIsland()
        if currentIslandFolder then
            local islandName = currentIslandFolder.Name
            local opts = getBaseNames()
            local totalMobs = #currentIslandFolder:GetChildren()
            Dropdown:Refresh(opts, true)
            StatusLabel:Set("‚úÖ " .. islandName .. " | " .. #opts .. " types | " .. totalMobs .. " mobs")
            Rayfield:Notify({
                Title = islandName .. " DETECTED!",
                Content = totalMobs .. " mobs ready",
                Duration = 5
            })
        else
            StatusLabel:Set("‚ùå NO ISLAND - Check F9 distances")
            Rayfield:Notify({Title = "No close island (see console)", Duration = 5})
        end
    end,
})

Rayfield:LoadConfiguration()
